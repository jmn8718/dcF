/bbva-tiles: 
    /{latitude}/{longitude}: 
        uriParameters: 
            latitude: 
                description: |
                    Latitude of the tile to be retrieved. Latitude will be rounded to the nearest "two and a half decimal" point, e.g. 19.366 to 19.365, 19.311 to 19.310, 19.36858 to 19.370...
                type: integer
                required: true
                repeat: false
                example: "20.68"
            longitude: 
                description: |
                    Longitude of the tile to be retrieved. Longitude will be rounded to the nearest "two and a half decimal" point, e.g. -99.136 to -99.135, -99.139 to -99.140, 19.361012 to 19.360...
                type: integer
                required: true
                repeat: false
                example: "-103.365"
        /basic_stats: 
            get: 
                description: Gets the measurement of customer loyalty distribution.
                headers: 
                    Accept: 
                        description: Request format
                        type: string
                        required: false
                        repeat: false
                        enum: [application/json]
                        default: application/json
                        example: application/json
                    Content-Type: 
                        description: Body response format
                        type: string
                        required: false
                        repeat: false
                        enum: [application/json]
                        default: application/json
                        example: application/json
                queryParameters: 
                    "$page_size": 
                        description: Number of items per page
                        type: integer
                        required: false
                        repeat: false
                        example: "10"
                    "$page_key": 
                        description: Key of the first element to be retrieved
                        type: string
                        required: false
                        repeat: false
                        example: "201403"
                    profile_name: 
                        description: the profile name
                        type: string
                        required: false
                        repeat: false
                        example: admin
                    category: 
                        description: Subcategory name of merchant (all or subcategory id)
                        type: string
                        required: false
                        repeat: false
                        example: es_hotelservices
                    channel: 
                        description: |
                            Payment channel of the transaction (pos,bbva_pos,opos,bbva_opos,all)
                        type: string
                        required: false
                        repeat: false
                        enum: [pos, bbva_pos, opos, bbva_opos, all]
                        example: opos
                    cards: 
                        description: the card owner type
                        type: string
                        required: false
                        repeat: false
                        enum: [bbva, all]
                        example: bbva
                    date_min: 
                        displayName: date_max
                        description: |
                            Oldest date to be retrieved. Statistics returned by this request will be bounded by this parameter and date_max
                        type: string
                        required: false
                        repeat: false
                        pattern: YYYMMDD
                        maxLength: 6
                        default: "20140101"
                        example: "20140101"
                    date_max: 
                        displayName: date_min
                        description: |
                            Most recent date to be retrieved. Statistics returned by this request will be bounded by this parameter and date_min
                        type: string
                        required: false
                        repeat: false
                        pattern: YYYMMDD
                        maxLength: 6
                        default: "20140331"
                        example: "20140331"
                    group_by: 
                        description: Time aggregation period
                        type: string
                        required: false
                        repeat: false
                        enum: [month]
                        default: month
                        example: month
                    app_id: 
                        displayName: app_id
                        description: Genoa Security Parameter
                        type: string
                        required: true
                        repeat: false
                    app_key: 
                        displayName: app_key
                        description: Genoa Security Parameter
                        type: string
                        required: true
                        repeat: false
                responses: 
                    "200": 
                        description: |
                            * Result:
                                * Code: HTTP Status
                                * Info: Additional information
                            * Data:
                                * Stats: statistics array
                                    * Date: temporal unit reference for the provided statistics
                                    * Avg: transaction average
                                    * Max: maximum transaction
                                    * Min: minimum transaction
                                    * Std: transaction standard deviation
                                    * Mode: transaction mode
                                    * Peak Txs Day: day with maximum number of transactions
                                    * Valley Txs Day: day with minimum number of transactions
                                    * Peak Txs Hour: hour with maximum number of transactions
                                    * Valley Txs Hour: hour with minimum number of transactions
                                    * Cards: number of cards
                                    * Txs: number of transactiona
                                    * Merchants: number of merchants
                        body: 
                            application/json: 
                                example: |
                                    {
                                        "result": {
                                            "code": 200,
                                            "info": "OK"
                                        },
                                        "data": {
                                            "stats": [
                                                {
                                                    "date": "201401",
                                                    "avg": 12,
                                                    "max": 12,
                                                    "min": 12,
                                                    "std": 12,
                                                    "mode": 12,
                                                    "peak_txs_day": 12,
                                                    "valley_txs_day": 12,
                                                    "peak_txs_hour": 12,
                                                    "valley_txs_hour": 12,
                                                    "cards": 12,
                                                    "txs": 12,
                                                    "merchants": 12
                                                },
                                                {
                                                    "date": "201402",
                                                    "avg": 7,
                                                    "max": 7,
                                                    "min": 7,
                                                    "std": 7,
                                                    "mode": 7,
                                                    "peak_txs_day": 7,
                                                    "valley_txs_day": 7,
                                                    "peak_txs_hour": 7,
                                                    "valley_txs_hour": 7,
                                                    "cards": 7,
                                                    "txs": 7,
                                                    "merchants": 7
                                                }            
                                            ]
                                        }
                                    }
                    "206": 
                        description: |
                            * Result:
                                * Code: HTTP Status
                                * Info: Additional information
                            * Paging:
                                * Page size: number of items in this page
                                * Links: link to other pages
                                    * Self: this page
                                    * First: first page for this range
                                    * Previous: previous page for this range
                                    * Next: next page for this range
                                    * Last: last page for this range
                            * Data:
                                * Stats: statistics array
                                    * Date: temporal unit reference for the provided statistics
                                    * Avg: payment average
                                    * Max: maximum payment
                                    * Min: minimum payment
                                    * Std: payment standard deviation
                                    * Mode: payment mode
                                    * Peak Payments Day: day with maximum number of payments
                                    * Valley Payments Day: day with minimum number of payments
                                    * Peak Payments Hour: hour with maximum number of payments
                                    * Valley Payments Hour: hour with minimum number of payments
                                    * Num. Cards: number of cards
                                    * Num. Payments: number of payments
                                    * Num. Merchants: number of merchants
                        body: 
                            application/json: 
                                example: |
                                    {
                                        "result": {
                                            "code": 206,
                                            "info": "Partial Content"
                                        },
                                        "paging": {
                                            "page_size": 2,
                                            "links": {
                                                "first": {
                                                    "href": "https://hostname/tiles/75.5/78.7/basic_stats?date_min=201401&date_max=201412&group_by=month&cards=bbva&category=es_moda&country=es&$page_size=2&profile_name=admin"
                                                },
                                                "self": {
                                                    "href": "https://hostname/tiles/75.5/78.7/basic_stats?date_min=201401&date_max=201412&group_by=month&cards=bbva&category=es_moda&country=es&$page_size=2&profile_name=admin"
                                                },
                                                "next": {
                                                    "href": "https://hostname/tiles/75.5/78.7/basic_stats?date_min=201401&date_max=201412&group_by=month&cards=bbva&category=es_moda&country=es&$page_size=2&profile_name=admin&$page_key=201403"
                                                }
                                            }
                                        },
                                        "data": {
                                            "stats": [
                                                {
                                                    "date": "201401",
                                                    "avg": 12,
                                                    "max": 12,
                                                    "min": 12,
                                                    "std": 12,
                                                    "mode": 12,
                                                    "peak_txs_day": 12,
                                                    "valley_txs_day": 12,
                                                    "peak_txs_hour": 12,
                                                    "valley_txs_hour": 12,
                                                    "cards": 12,
                                                    "txs": 12,
                                                    "merchants": 12
                                                },
                                                {
                                                    "date": "201402",
                                                    "avg": 7,
                                                    "max": 7,
                                                    "min": 7,
                                                    "std": 7,
                                                    "mode": 7,
                                                    "peak_txs_day": 7,
                                                    "valley_txs_day": 7,
                                                    "peak_txs_hour": 7,
                                                    "valley_txs_hour": 7,
                                                    "cards": 7,
                                                    "txs": 7,
                                                    "merchants": 7
                                                }
                                            ]
                                        }
                                    }
